.PHONY: get_started

get_started:
	@echo 'Please read README.STARTING_P2 as soon as this completes.'
	@sleep 2
	@echo 'Please read README.STARTING_P2 as soon as this completes.'
	@sleep 2
	@echo 'Please read README.STARTING_P2 as soon as this completes.'
	@sleep 2
	./update.sh
	$(MAKE)
	@echo '' ; echo 'Please read README.STARTING_P2 now.'
	@echo '' ; echo 'Please read README.STARTING_P2 now.'
	@echo '' ; echo 'Please read README.STARTING_P2 now.'
